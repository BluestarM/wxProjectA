page {
  background: #F9F9F9;
}

.page_container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  box-sizing: border-box;
  margin-bottom: 3.2vw;
  padding: 40rpx;
  background: #F9F9F9;
}

.home_top {
  display: flex;
  flex-direction: column;
}

.home_title {}

.home_top_bg {
  position: relative;
  width: 100%;
  height: 408rpx;
  background: #F78631;
}

.home_top_bg::before {
  content: "";
  width: 100%;
  height: 40rpx;
  background: #F9F9F9;
  position: absolute;
  bottom: 0;
  left: 0;
  display: block;
  border-radius: 40rpx 40rpx 0 0;
}

.home_content {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}

.home_content_list {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  margin-top: 40rpx;
}

.list_img {
  display: block;
  width: 88rpx;
  height: 84rpx;
}

.home_content_list .home_list_info {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 76%;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 20rpx;
  cursor: pointer;
  background: #eee;
  border-radius: 8rpx;
  box-shadow: 0px 3rpx 50rpx rgba(0, 0, 0, 0.04);
  font-size: 32rpx;
  color: #000;
}

.home_content_list .home_list_info image {
  display: block;
  width: 20rpx;
  height: 40rpx;
}

.car_active {
  color: #fff !important;
  background: #2294F9 !important;
}

.home_main {
  display: flex;
  flex-direction: column;
  width: 90%;
  margin: 150rpx auto 0;
}

.news_img {
  display: flex;
  position: relative;
}

.share_coun {
  width: 100%;
  height: 288rpx;
  margin-top: 20rpx;
}

.shareWechat_cover {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}

.shareWechat_cover button {
  height: 100%;
}


.picker-wrap {
  position: absolute;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  bottom: 0rpx;
  background: #fff;
  border-radius: 20rpx 20rpx 0 0;
  z-index: 999;
}

.buycar_dialod {
  position: relative;
  width: 100%;
  max-height: 600rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  overflow-y: scroll;
}

.carlist_content {
  display: flex;
  flex-direction: column;
  width: 100%;
}

.sel_list {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 110rpx;
  line-height: 110rpx;
  border-bottom: 1px solid #ccc;
}